<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bDetails">
	
	<insert id="Insert" parameterType="BDetail">
		INSERT INTO b_details(bibe_no,bibe_avb,bibe_brand)
		VALUES (#{bibeNo},#{bibeAvb},#{bibeBrand})
	</insert>
	
</mapper>